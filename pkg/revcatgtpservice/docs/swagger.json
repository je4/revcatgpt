{
    "swagger": "2.0",
    "info": {
        "description": "Custom GPT Metadata for Revcat",
        "title": "RevcatGPT API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "JÃ¼rgen Enge",
